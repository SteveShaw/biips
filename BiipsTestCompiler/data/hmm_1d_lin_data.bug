var x[1,t.max+1], y[1, t.max], x.true[1,t.max+1]

data{
  x.true[,1] ~ dnormvar(mean.x.init, var.x.init)
  for (t in 2:t.max+1)
  {
    x.true[,t] ~ dnormvar(x.true[,t-1], var.x)
    y[,t-1] ~ dnormvar(x.true[,t], var.y)
  }
}

model
{
  x[,1] ~ dnormvar(mean.x.init, var.x.init)
  for (t in 2:t.max+1)
  {
    x[,t] ~ dnormvar(x[,t-1], var.x)
    y[,t-1] ~ dnormvar(x[,t], var.y)
  }
}